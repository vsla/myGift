generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int     @id @default(autoincrement())
  name        String
  imgUrl      String
  gifted      Boolean @default(false)
  giftedBy    String?
  description String
  category    String
  price       Float
  productLink String
}
